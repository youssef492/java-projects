    private void btnadddata1ActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:

        JLabel imagelabel = new JLabel();
        ImageIcon imageicon  = new ImageIcon(selectedImagePath);
        imagelabel.setIcon(imageicon);
      /////////////////////////////////////////////////////////////////////////////////  
        String name = txtName.getText();
        String email = txtEmail.getText();
        String phone = txtPhone.getText();     
         SimpleDateFormat dFormat = new SimpleDateFormat("yyyy-MM-dd");
        String birthdate  = dFormat.format(jDateChooser1.getDate());
       ////////////////////////////////////////////////////////////////////////////// 
        
        
        if(name.isEmpty()||email.isEmpty()||phone.isEmpty()){
        JOptionPane.showMessageDialog(this, "please enter all data!");
        }else{
           
            if( txtName.getText()!=("^[a-zA-Z][a-zA-Z\\s]+$") && txtName.getText().length() < 20){
                  // "^[a-zA-Z0-9+_.-]+@[a-zA-Z0-9.-]+$"
                  
            }else{JOptionPane.showMessageDialog(this, "note : name not greater than 20");} 
                if(txtEmail.getText()!= "^[a-zA-Z0-9+_.-]+@[a-zA-Z0-9.-]+$"){
               

            }else { JOptionPane.showMessageDialog(this, "Email not valid");}
            
                
              if(txtPhone.getText().length()== 11 && txtPhone.getText().charAt(0)=='0' && txtPhone.getText().charAt(1)=='1' && (txtPhone.getText().charAt(2)=='0' || txtPhone.getText().charAt(2)=='1' || txtPhone.getText().charAt(2)=='2' || txtPhone.getText().charAt(2)=='5')){
                

            }else{JOptionPane.showMessageDialog(this, "phone number not valid");}  
            
         DefaultTableModel model = (DefaultTableModel) table1.getModel();
        model.addRow(new Object[] {name,email,phone,birthdate,imagelabel});
        JOptionPane.showMessageDialog(this, "data added succesfully..!!");
                clearFields();
        }
    }                                           

  
    private void btnclearActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        clearFields();
    }                                        

    private void txtdeleteallActionPerformed(java.awt.event.ActionEvent evt) {                                             
        // TODO add your handling code here:
        int i = table1.getRowCount();
        DefaultTableModel tblModel = (DefaultTableModel) table1.getModel();
        for (int j = 0; j < i; j++) {
            tblModel.removeRow(0);
        }
    }                                            

    private void txtdeleteActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        DefaultTableModel tblModel = (DefaultTableModel) table1.getModel();
        if(table1.getSelectedRowCount()==1){
            tblModel.removeRow(table1.getSelectedRow());
        }else{
            if(table1.getRowCount()==0){
                JOptionPane.showMessageDialog(this, "table is empty");
            }else{
                JOptionPane.showMessageDialog(this, "please select single row for delete");
            }
        }
    }                                         

    private void jTextField1ActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
        Search();
    }                                           

