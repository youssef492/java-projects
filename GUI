    }                                        

    private void photoActionPerformed(java.awt.event.ActionEvent evt) {                                      

        try {
//filter image
JFileChooser chooser  = new JFileChooser();
chooser.showOpenDialog(null);
File f = chooser.getSelectedFile();
String path = f.getAbsolutePath();
BufferedImage bi = ImageIO.read(new File(path));
Image img = bi.getScaledInstance(134 , 172 , Image.SCALE_SMOOTH);
ImageIcon icon = new ImageIcon(img);
jLablephoto.setIcon(icon);
        } catch (IOException ex) {
            Logger.getLogger(NewJFramelogin2.class.getName()).log(Level.SEVERE, null, ex);
        }
    }                                     

    private void btnupdateActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        DefaultTableModel model = (DefaultTableModel) table1.getModel();
        if(table1.getSelectedRowCount() == 1){
        String name = txtName.getText();
        String Email = txtEmail.getText();
        String Phone  = txtPhone.getText();
        model.setValueAt(name, table1.getSelectedRow(), 0);
        model.setValueAt(Email, table1.getSelectedRow(), 1);
        model.setValueAt(Phone, table1.getSelectedRow(), 2);
        
        JOptionPane.showMessageDialog(this, "Update succssefully...!!");
        
        }else{
        
        if(table1.getRowCount()==0){
        JOptionPane.showMessageDialog(this, "table is empty.....");
        }else{JOptionPane.showMessageDialog(this, "please select single row for update..");}
        }
    }                                         

